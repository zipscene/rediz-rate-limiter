{
	"whitelist": [
		"es6.arrowFunctions",
		"es6.blockScoping",
		"es6.classes",
		"es6.constants",
		"es6.destructuring",
		"es6.forOf",
		"es6.modules",
		"es6.parameters",
		"es6.properties.computed",
		"es6.properties.shorthand",
		"es6.spread",
		"es6.templateLiterals",
		"strict",
		"es6.objectSuper",
		"runtime"
	],
	"blacklist": [
		"es6.tailCall",
		"flow",
		"react",
		"reactCompat",
		"regenerator",
		"es6.regex.sticky",
		"es6.regex.unicode",
		"es7.comprehensions",
		"es7.classProperties",
		"es7.decorators",
		"es7.exportExtensions",
		"es7.exponentiationOperator",
		"es7.objectRestSpread"
	],
	"loose": [
		"es6.properties.computed",
		"es6.forOf",
		"es6.objectSuper"
	],
	"modules": "common",
	"sourceMap": "inline",
	"compact": false,
	"comments": true
}
