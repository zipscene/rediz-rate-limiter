{
  "name": "rediz-rate-limiter",
  "version": "0.0.1",
  "description": "Redis-based rate limiting",
  "main": "./dist/lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zipscene/rediz-rate-limiter.git"
  },
  "keywords": [
    "redis",
    "rediz",
    "rate-limit"
  ],
  "scripts": {
    "build": "rm -rf dist && broccoli build dist; chmod -f u+x dist/bin/*; rm -rf tmp",
    "docs": "yuidoc .",
    "lint": "eslint . || exit 0",
    "prepublish": "npm run build && npm run docs",
    "unit": "npm run build && mocha ./dist/test/setup.js ./dist/test/unit --recursive -R spec",
    "integration": "npm run build && mocha ./dist/test/setup.js ./dist/test/integration --recursive -R spec",
    "test": "npm run unit; npm run integration"
  },
  "dependencies": {
    "babel-runtime": "6.23.0",
    "lodash": "^4.17.4",
    "xerror": "^1.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "broccoli": "^0.16.9",
    "broccoli-babel-transpiler": "^5.6.2",
    "broccoli-cli": "^1.0.0",
    "broccoli-funnel": "^1.2.0",
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-static-compiler": "^0.2.2",
    "broccoli-stew": "^1.4.2",
    "chai": "^3.5.0",
    "es6-promise": "^4.1.0",
    "eslint": "^3.19.0",
    "eslint-config-zipscene": "^0.2.3",
    "mocha": "^3.3.0",
    "pasync": "^1.4.1",
    "rediz": "^1.0.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "source-map-support": "^0.4.14",
    "yuidocjs": "^0.10.2"
  }
}
